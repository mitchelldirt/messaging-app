package pages


templ Home(name string) {
    <head>
        <title>Hello, {name}!</title>
        <link rel="stylesheet" href="/pages/home/home.css">
    </head>
    <div>
        <h1>Hello, {name}!</h1>
        <p>Welcome to the templ example.</p>
        <p>
            Can I actually easily edit this now????
        </p>
        <p>
            I can!
        </p>
        <marquee>
            it's alive!!!!!
        </marquee>
    </div>
}